import java.util.Scanner;


public class ConsoleInput {

	public static void main(String[] args) {
		
		Scanner scan = new Scanner(System.in);
		
		String firstWord = scan.next("\\w+");
		System.out.println(firstWord);
		String secondWord = scan.next("\\w+");
		System.out.println(secondWord);
		
		int numInt = scan.nextInt();
		System.out.println(numInt);
		
		double numDouble1 = scan.nextDouble();
		System.out.println(numDouble1);
		double numDouble2 = scan.nextDouble();
		System.out.println(numDouble2);
	}

}
